@model IEnumerable<MineHControlReports.Models.goal>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>

<table id="myTable" class="display">
    <thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.KpiId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StartDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EndDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TypeEntity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EntityId)
        </th>
        <th></th>
    </tr>

</thead>
    <tbody>
@foreach (var item in Model) {
    <tr>
        <td>
               @{
    string parameterValue2 = item.KpiGet();
              }
            @Html.DisplayFor(modelItem => parameterValue2 )           
            
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StartDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EndDate)
        </td>
        <td>
            @{
    string parameterValue = item.TypeEntityGet();
              }
            @Html.DisplayFor(modelItem => parameterValue )           
            
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EntityId)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}
    

</tbody>
</table>
